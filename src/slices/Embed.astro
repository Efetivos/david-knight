---
import { asHTML } from "@prismicio/helpers";
const { slice } = Astro.props;
---
<section class="embed">
    <div class="embed__hold e-hold">
        <div class="embed__embed w-embed w-iframe"><iframe width="100%" height="100%" src={slice.primary.embed_content} title="" frameborder="0" allow="accelerometer; autoplay; clipboard-write; encrypted-media; gyroscope; picture-in-picture; web-share" referrerpolicy="strict-origin-when-cross-origin" allowfullscreen=""></iframe></div>
    </div>
</section>
